div.masthead 
	div.navbar
		table
			tr
				td
					a(href="/")
						img.logo(src="/images/fclogo.png")
				td.menu
					a(href="/") Home
					a(href="/press") Press
					a(href="/schools") Courses
					a(href="/archive") Archive

					- if( user && user.loggedIn )
						a(href="/profile") Profile
						span(class='sub_menu')
							a( href='/logout' ) Logout
					- else
						a(href="/register") Create an Account
						span(class='sub_menu')
							a(href="/login") Login
