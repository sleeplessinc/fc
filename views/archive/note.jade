div#wrapper
	div#header
		table
			tr
				td.lilogo
					a(href="/")
						img.logo(height="30", src="/images/fclogo.png")
				td
					div#breadcrumb
						div.crumb
							- course.section = course.section.replace(/\)/, '')
							strong.label Course: #{ course.section + ' - ' + course.name }
						div.crumb
							strong.label Note: #{ note.topic || note.id }

	div#staticNote
		!{ note.text || 'This note is empty.' }

	div( style = 'clear: both; height: 1px;' )

